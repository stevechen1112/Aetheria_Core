{
  "system_info": {
    "name": "姓名學五格剖象法",
    "name_en": "Chinese Name Analysis (Five Grid Method)",
    "version": "1.0.0",
    "description": "基於康熙字典筆畫的姓名學分析系統"
  },
  "five_grids": {
    "天格": {
      "name_en": "Heaven Grid",
      "description": "代表先天運勢，由姓氏筆畫決定",
      "calculation": "單姓：姓氏筆畫+1；複姓：姓氏總筆畫",
      "influence": "影響人生前半期（1-20歲），代表祖蔭、父母緣"
    },
    "人格": {
      "name_en": "Personality Grid",
      "description": "代表主運，是姓名學中最重要的格",
      "calculation": "姓氏最後一字 + 名字第一字的筆畫",
      "influence": "影響人生中期（21-40歲），代表性格、才能、事業"
    },
    "地格": {
      "name_en": "Earth Grid",
      "description": "代表前運，影響青年時期",
      "calculation": "名字所有字的筆畫總和；單名則+1",
      "influence": "影響人生前期（1-35歲），代表家庭、婚姻、子女"
    },
    "外格": {
      "name_en": "External Grid",
      "description": "代表副運，影響人際關係",
      "calculation": "總格 - 人格 + 1",
      "influence": "影響社交、人際關係、外在環境"
    },
    "總格": {
      "name_en": "Total Grid",
      "description": "代表後運，影響晚年",
      "calculation": "姓名所有字的筆畫總和",
      "influence": "影響人生後期（40歲後），代表晚年運勢、總體成就"
    }
  },
